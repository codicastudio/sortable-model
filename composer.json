{
    "name": "codicastudio/sortable-model",
    "description": "A random Codica Studio package.",

    "homepage": "https://github.com/codicastudio/sortable-model",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/database": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^9.0",
        "orchestra/testbench": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\EloquentSortable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\EloquentSortable\\Test\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=no",
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\EloquentSortable\\EloquentSortableServiceProvider"
            ]
        }
    }
}
